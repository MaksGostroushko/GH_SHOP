<%= form_for @product do |f| %>
  <%# render "shared/errors", object: @product %>
  <div class="form-control">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="form-control">
    <%= f.label :price %>
    <%= f.text_field :price %>
  </div>

  <div class="form-control">
    <%= f.label :result %>
    <%= f.text_field :result %>
  </div>

  <div class="form-control">
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

<div class="form-control">
  <%= f.submit 'Save', class: 'btn btn-success' %>
</div>

<% end %>
